<p id="notice"><%= notice %></p>

<h1>Winners</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>prize</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @winners.each do |winner| %>
      <tr>
        <td><%= winner.number.user.name %></td>
        <td><%= winner.prize.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
